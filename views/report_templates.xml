<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="web.external_layout_standard">
            <div class="header">
                <div class="row">
                    <div class="col-xs-6" name="company_address">
                        <strong t-field="company.partner_id"/><br/>
                        <span t-field="company.street"/>
                        <span t-field="company.street2"/>
                        <span t-field="company.city"/>
                        <span t-field="company.country_id"/>
                        <div t-field="company.company_registry"/>
                        <span>Điện thoại: </span><span t-field="company.phone"/>
                        <span style="padding:0 5px"/>
                        <span>Website: </span><span t-field="company.website"/>
                        <div style="border-bottom: 1px solid black;"/>
                    </div>
                </div>
            </div>

            <div class="article o_report_layout_standard">
                <t t-raw="0" />
            </div>

            <div class="footer">
                    <div class="text-center text-muted">
                        Page: <span class="page"/> / <span class="topage"/>
                    </div>

            </div>
        </template>
    </data>
</odoo>
